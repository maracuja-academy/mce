  src: url('fonts/SFUIDisplay-Regular.eot?#iefix') format('embedded-opentype'),  url('fonts/SFUIDisplay-Regular.otf')  format('opentype'),
	     url('fonts/SFUIDisplay-Regular.woff') format('woff'), url('fonts/SFUIDisplay-Regular.ttf')  format('truetype'), url('fonts/SFUIDisplay-Regular.svg#SFUIDisplay-Regular') format('svg');
  font-weight: normal;
  font-style: normal;
}

